    <declare-styleable name="calendar_cell">
        <attr name="state_selectable" format="boolean" />
        <attr name="state_current_month" format="boolean" />
        <attr name="state_today" format="boolean" />
        <attr name="state_range_first" format="boolean" />
        <attr name="state_range_middle" format="boolean" />
        <attr name="state_range_last" format="boolean" />
    </declare-styleable>
    <!-- From: file:/Users/gravener/AndroidStudioProjects/happyschedule/libraries/AndroidTimesSquare/library/res/values/dimens.xml -->
    <eat-comment />

    <dimen name="calendar_day_headers_paddingbottom">4dp</dimen>
    <dimen name="calendar_month_title_bottommargin">4dp</dimen>
    <dimen name="calendar_month_topmargin">16dp</dimen>
    <dimen name="calendar_text_medium">18sp</dimen>
    <dimen name="calendar_text_small">14sp</dimen>
    <!-- From: file:/Users/gravener/AndroidStudioProjects/happyschedule/libraries/AndroidTimesSquare/library/res/values/strings.xml -->
    <eat-comment />

    <string name="day_name_format">EEE</string>
    <string name="invalid_date">Date must be between %1$s and %2$s.</string>
    <string name="month_name_format">MMMM yyyy</string>
    <!-- From: file:/Users/gravener/AndroidStudioProjects/happyschedule/libraries/AndroidTimesSquare/library/res/values/styles.xml -->
    <eat-comment />

    <style name="CalendarCell">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textAlignment">center</item>
    </style>

    <style name="CalendarCell.CalendarDate">
        <item name="android:textSize">@dimen/calendar_text_medium</item>
        <item name="android:background">@drawable/calendar_bg_selector</item>
        <item name="android:textColor">@color/calendar_text_selector</item>
        <item name="android:clickable">true</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="CalendarCell.DayHeader">
        <item name="android:textColor">@color/calendar_text_active</item>
        <item name="android:textSize">@dimen/calendar_text_small</item>
    </style>

    <style name="CalendarTitle">
        <item name="android:textSize">@dimen/calendar_text_medium</item>
        <item name="android:textColor">@color/calendar_text_active</item>
        <item name="android:textStyle">bold</item>
    </style>

</resources>
